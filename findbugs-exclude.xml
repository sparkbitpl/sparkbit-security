<FindBugsFilter>
    <Match>
        <Class name="pl.sparkbit.security.rest.RestAuthenticationProvider"/>
        <Bug pattern="BC_UNCONFIRMED_CAST"/>
    </Match>

    <Match>
        <Class name="pl.sparkbit.security.login.AuthTokenDTO"/>
        <Field name="authToken"/>
        <Bug pattern="URF_UNREAD_FIELD"/>
    </Match>

    <Match>
        <Class name="pl.sparkbit.security.rest.RestUserDetails"/>
        <Field name="roles"/>
        <Bug pattern="UWF_UNWRITTEN_FIELD"/>
    </Match>

    <Match>
        <Bug pattern="SE_NO_SERIALVERSIONID"/>
    </Match>
</FindBugsFilter>
