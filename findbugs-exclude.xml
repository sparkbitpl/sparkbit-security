<FindBugsFilter>
    <Match>
        <Class name="pl.sparkbit.security.rest.user.RestAuthenticationProvider"/>
        <Bug pattern="BC_UNCONFIRMED_CAST"/>
    </Match>

    <Match>
        <Class name="pl.sparkbit.security.login.AuthTokenDTO"/>
        <Field name="authToken"/>
        <Bug pattern="URF_UNREAD_FIELD"/>
    </Match>

    <Match>
        <Class name="pl.sparkbit.security.rest.user.RestUserDetails"/>
        <Field name="roles"/>
        <Bug pattern="UWF_UNWRITTEN_FIELD"/>
    </Match>

    <Match>
        <Class name="pl.sparkbit.security.social.GoogleAuthenticationProvider"/>
        <Method name="authenticate"/>
        <Bug pattern="BC_UNCONFIRMED_CAST"/>
    </Match>

    <Match>
        <Class name="pl.sparkbit.security.social.TwitterAuthenticationProvider"/>
        <Method name="authenticate"/>
        <Bug pattern="BC_UNCONFIRMED_CAST"/>
    </Match>

    <Match>
        <Class name="pl.sparkbit.security.social.FacebookAuthenticationProvider"/>
        <Method name="authenticate"/>
        <Bug pattern="BC_UNCONFIRMED_CAST"/>
    </Match>

    <Match>
        <Bug pattern="SE_NO_SERIALVERSIONID"/>
    </Match>

    <Match>
        <Class name="pl.sparkbit.security.password.PHPass"/>
        <Bug pattern="DM_DEFAULT_ENCODING"/>
    </Match>
</FindBugsFilter>
